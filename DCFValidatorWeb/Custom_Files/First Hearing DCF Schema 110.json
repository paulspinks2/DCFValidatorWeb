{
  "description": "First Hearing Digital Case File 1.1.0",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "pti_urn": {
      "type": "string",
      "pattern": "[A-Z]{4}[0-9]{5}[0-9]{2}(-[A-Z]{2})?"
    },
    "type": {
      "enum": [ "FirstHearingDCF", "BobDCF" ]
    },
    "associated_case_urns": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "[A-Z]{4}[0-9]{5}[0-9]{2}(-[A-Z]{2})?",
        "uniqueItems": true,
        "minItems": 1
      }
    },
    "version": {
      "type": "number",
      "minimum": 1
    },
    "case_markers": {
      "type": "array",
      "items": {
        "enum": [
          "AssetRecovery",
          "ChildAbuse",
          "CrimeAgainstAnOlderPerson",
          "DrugInterventionProgramme",
          "DisabilityHateCrime",
          "DomesticViolence",
          "Fatality",
          "ForcedMarriage",
          "Homophobic",
          "HonourCrime",
          "IdentifiedVictim",
          "PoliceComplaints",
          "MediaInterest",
          "Rape",
          "RacistCrime",
          "ReligiousCrime",
          "Transgender"
        ]
      }
    },
    "brief_description_of_case": {
      "type": "string",
      "maximum": 1200
    },
    "defendants": {
      "type": "array",
      "items": [ { "$ref": "#/definitions/Defendant" } ],
      "minItems": 1,
      "uniqueItems": true
    },
    "witnesses": {
      "type": "array",
      "items": {
        "anyOf": [
          { "$ref": "#/definitions/StatementWitness" },
          { "$ref": "#/definitions/Victim" },
          { "$ref": "#/definitions/NgapOtherWitness" },
          { "$ref": "#/definitions/ExpertWitness" }
        ]
      },
      "minItems": 1,
      "uniqueItems": true
    },
    "common_law_disclosure": {
      "type": "object",
      "oneOf": [
        { "$ref": "#/definitions/CommonLawNotForDisclosure" },
        { "$ref": "#/definitions/CommonLawForDisclosure" }
      ]
    },
    "likely_case_progression": {
      "enum": [
        "summary_only_or_either_way_and_likely_to_remain_in_the_magistrates_court",
        "indictable_only_or_either_way_and_likely_to be_heard_in_the_crown_court"
      ]
    },
    "multimedia_evidence": {
      "type": "array",
      "items": { "$ref": "#/definitions/Mme" }
    },
    "is_hearsay": {
      "type": "boolean"
    },
    "hearsay_details": {
      "type": "string"
    },
    "expert_evidence": {
      "type": "boolean"
    },
    "test_code": {
      "enum": [
        "threshold_test",
        "full_code_test"
      ]
    },
    "pca_cps": {
      "type": "boolean"
    },
    "safeguarding_assessment": {
      "$ref": "#/definitions/SafeguardingAssessment"
    },
    "property": {
      "$ref": "#/definitions/Property"
    },
    "officer_in_the_case": {
      "$ref": "#/definitions/PersonName"
    },
    "signatory_name": {
      "$ref": "#/definitions/PersonName"
    },
    "date": {
      "description": "ISO 8601 Conformant Date",
      "type": "string"
    },
    "signatory_rank": {
      "type": "string"
    }
  },
  "required": [
    "pti_urn",
    "type",
    "version",
    "brief_description_of_case",
    "defendants",
    "witnesses",
    "common_law_disclosure",
    "expert_evidence",
    "test_code",
    "pca_cps",
    "officer_in_the_case",
    "signatory_name",
    "date",
    "signatory_rank"
  ],
  "additionalProperties": false,

  "definitions": {

    "CommonLawNotForDisclosure": {
      "type": "object",
      "properties": {
        "reporting_officer": {
          "$ref": "#/definitions/PersonName"
        },
        "reporting_officer_signature_date": {
          "description": "ISO 8601 Conformant Date",
          "type": "string"
        }
      },
      "required": [
        "reporting_officer",
        "reporting_officer_signature_date"
      ],
      "additionalProperties": false
    },

    "CommonLawForDisclosure": {
      "type": "object",
      "properties": {
        "reporting_officer": {
          "type": "string"
        },
        "reporting_officer_signature_date": {
          "description": "ISO 8601 Conformant Date",
          "type": "string"
        },
        "details_of_relevant_unused_material": {
          "type": "string"
        }
      },
      "required": [
        "reporting_officer",
        "reporting_officer_signature_date",
        "details_of_relevant_unused_material"
      ],
      "additionalProperties": false
    },

    "SafeguardingAssessment": {
      "type": "object",
      "properties": {
        "assessment_completed": {
          "type": "boolean"
        },
        "reasons_not_completed": {
          "type": "string"
        },
        "safeguarding_level": {
          "enum": [
            "moderate",
            "moderate_serious",
            "serious",
            "severe"
          ]
        }

      },
      "required": [
        "assessment_completed"
      ],
      "additionalProperties": false
    },

    "Mme": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "date": {
          "description": "ISO 8601 Conformant Date",
          "type": "string"
        },
        "end_date": {
          "description": "ISO 8601 Conformant Date",
          "type": "string"
        },
        "type": {
          "enum": [
            "cctv",
            "facewatch",
            "body_worn_video",
            "surveillance_camera",
            "999_call"
          ]
        },
        "evidential_value": {
          "enum": [ true ]
        },
        "relied_upon_trial": {
          "type": "boolean"
        },
        "relied_upon_sentence": {
          "type": "boolean"
        },
        "description_of_what_is_contained_in_multimedia_evidence": {
          "type": "string"
        },
        "total_duration": {
          "description": "ISO 8601 extended format, accurate to 1 minute",
          "type": "string"
        },
        "url": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "date",
        "type",
        "evidential_value",
        "relied_upon_sentence",
        "relied_upon_trial",
        "description_of_what_is_contained_in_multimedia_evidence",
        "total_duration"

      ],
      "additionalProperties": false
    },

    "Defendant": {
      "type": "object",
      "properties": {
        "defendant_id": {
          "type": "string"
        },
        "name": {
          "$ref": "#/definitions/PersonName"
        },
        "date_of_birth": {
          "description": "ISO 8601 Conformant Date",
          "type": "string"
        },
        "arrest": {
          "$ref": "#/definitions/Arrest"
        },
        "conditional_caution_considered": {
          "enum": [ true ]
        },
        "cc_not_suitable_reasons": {
          "type": "array",
          "items": {
            "enum": [
              "offence_not_one_in_dpps_guidance_for_conditional_caution",
              "offence_not_admitted",
              "partial_defence_raised",
              "offence_whilst_on_bail",
              "liable_for_resentence_of_an_earlier_offence",
              "liable_for_prison_recall",
              "defendants_response_on arrest_co operation",
              "reoffending_risk_ offending history",
              "reoffending_risk_ response_to_sentence_bail",
              "reoffending_risk_ not_reduced_by_conditions"
            ]
          }
        },
        "offences": {
          "type": "array",
          "minItems": 1,
          "items": {
            "oneOf": [
              { "$ref": "#/definitions/GenericOffences" },
              { "$ref": "#/definitions/RetailTheftOffences" },
              { "$ref": "#/definitions/AssaultOffences" }
            ]
          }
        },
        "occupation": {
          "type": "string"
        },
        "class_a_drug_test_provided": {
          "type": "boolean"
        },
        "class_a_drug_test_details": {
          "type": "string"
        },
        "accepts_drugs_result": {
          "type": "boolean"
        },
        "has_eec_passports": {
          "type": "boolean"
        },
        "eec_check_submitted": {
          "type": "boolean"
        },
        "eec_convictions_record_received": {
          "type": "boolean"
        },
        "foreign_national_offender": {
          "type": "boolean"
        },
        "breach_of_order": {
          "type": "boolean"
        },
        "breach_order_date_issued": {
          "description": "ISO 8601 Conformant Date",
          "type": "string"
        },
        "breach_order_court_issued": {
          "$ref": "#/definitions/OrganisationLocation"
        },
        "breach_order_title": {
          "$ref": "#/definitions/CourtOrderTitle"
        },
        "breach_order_title_other": {
          "type": "string"
        },
        "breach_order_lapse_date": {
          "description": "ISO 8601 Conformant Date",
          "type": "string"
        },
        "remand_application": {
          "$ref": "#/definitions/RemandApplication"
        },
        "breach_of_bail": {
          "$ref": "#/definitions/BreachOfBail"
        },
        "pnc_check_performed": {
          "type": "boolean"
        },
        "pnc_convictions": {
          "type": "boolean"
        },
        "court_order_applications": {
          "type": "array",
          "items": { "$ref": "#/definitions/CourtOrder" },
          "minItems": 1
        },
        "interpreter_required": {
          "type": "boolean"
        },
        "interpreter_language_or_dialect": {
          "type": "string"
        },
        "multimedia_evidence_response": {
          "type": "array",
          "items": {
            "oneOf": [
              { "$ref": "#/definitions/MmeRecordedResponse" },
              { "$ref": "#/definitions/MmeNotRecordedResponse" }
            ]
          }
        },
        "notice_to_provide_bad_character_evidence": {
          "type": "boolean"
        },
        "bad_character_details": {
          "type": "string"
        },
        "dealt_with_in_welsh": {
          "type": "boolean"
        },
        "person_remand_status": {
          "enum": [
            "custody",
            "unconditional_bail",
            "conditional_bail",
            "not_applicable"
          ]
        },
        "given_charges": {
          "type": "boolean"
        },
        "initiation_type": {
          "enum": [
            "charge",
            "requisition",
            "summons"
          ]
        },
        "signed_charge_sheet": {
          "description": "",
          "type": "boolean"
        },
        "date_sent": {
          "type": "string"
        },
        "parent_guardian_copy": {
          "type": "boolean"
        },
        "parent_guardian_date_sent": {
          "type": "string"
        },
        "bail_conditions_provided": {
          "type": "boolean"
        },
        "signed_for_bail": {
          "type": "boolean"
        },
        "bail_conditions": {
          "type": "array",
          "items": { "$ref": "#/definitions/BailCondition" }
        },
        "interview": {
          "type": "object",
          "oneOf": [
            { "$ref": "#/definitions/StandardInterview" },
            { "$ref": "#/definitions/NotInterviewed" }
          ]
        },
        "domestic_violence": {
          "$ref": "#/definitions/DomesticViolence"
        },
        "first_hearing_datetime": {
          "description": "ISO 8601 Conformant Date",
          "type": "string"
        },
        "first_hearing_location": {
          "$ref": "#/definitions/OrganisationLocation"
        }
      },
      "required": [
        "name",
        "date_of_birth",
        "offences",
        "conditional_caution_considered",
        "cc_not_suitable_reasons",
        "occupation",
        "has_eec_passports",
        "foreign_national_offender",
        "breach_of_order",
        "pnc_check_performed",
        "court_order_applications",
        "interpreter_required",
        "multimedia_evidence_response",
        "dealt_with_in_welsh",
        "person_remand_status",
        "given_charges",
        "initiation_type",
        "interview",
        "first_hearing_datetime",
        "first_hearing_location"
      ],
      "additionalProperties": false
    },

    "Arrest": {
      "properties": {
        "type": {
          "enum": [ "Arrest" ]
        },
        "date_time": {
          "type": "string"
        },
        "comments_on_arrest_and_caution": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "required": [
        "type",
        "date_time"
      ]
    },

    "RemandApplication": {
      "type": "object",
      "properties": {
        "substantial_grounds": {
          "type": "array",
          "items": { "$ref": "#/definitions/BailOpposition" }
        },
        "bail_conditions": {
          "type": "array",
          "items": { "$ref": "#/definitions/BailCondition" }
        }
      },
      "required": [
        "substantial_grounds",
        "bail_conditions"
      ],
      "additionalProperties": false
    },

    "BailOpposition": {
      "type": "object",
      "properties": {
        "opposition_ground": {
          "enum": [
            "breach_of_bail",
            "failed_to_surrender_to_custody",
            "commit_offences_on_bail",
            "interfere_with_witness_or_otherwise_obstrcut_the_course_of_justice",
            "for_the_defendants_own_protection",
            "for_the_defendants_own_welfare",
            "charged_with_or_convicted_of_a_violent_or_sexual_offence_equivalent_to_14_plus_years_imprisonment_in_the_case_of_an_adult",
            "the_only_way_to_protect_the_public_from_serious_harm",
            "to_prevent_the_commission_of_imprisonable_offences"
          ]
        },
        "justification": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "required": [
        "opposition_ground",
        "justification"
      ]
    },

    "BreachOfBail": {
      "type": "object",
      "properties": {
        "breach_details": {
          "type": "string"
        },
        "witness_statement_establishes_breach": {
          "type": "boolean"
        },
        "key_evidence": {
          "type": "string"
        },
        "existing_bail_breached": {
          "$ref": "#/definitions/ExistingBail"
        }
      },
      "additionalProperties": false,
      "required": [
        "breach_details",
        "witness_statement_establishes_breach",
        "key_evidence",
        "existing_bail_breached"
      ]
    },

    "ExistingBail": {
      "type": "object",
      "properties": {
        "original_charge_wording": {
          "type": "string"
        },
        "bailed_from": {
          "$ref": "#/definitions/OrganisationLocation"
        },
        "bailed_date": {
          "description": "ISO 8601 Conformant Date",
          "type": "string"
        },
        "to_appear_at": {
          "$ref": "#/definitions/OrganisationLocation"
        },
        "datetime_to_appear": {
          "description": "ISO 8601 Conformant Datetime",
          "type": "string"
        },
        "list_of_conditions": {
          "type": "array",
          "items": { "$ref": "#/definitions/BailCondition" }
        }
      },
      "required": [
        "original_charge_wording",
        "bailed_from",
        "bailed_date",
        "to_appear_at",
        "datetime_to_appear"
      ],
      "additionalProperties": false
    },

    "BailCondition": {
      "properties": {
        "grounds": {
          "type": "array",
          "items": {
            "enum": [
              "failing_to_surrender_to_custody",
              "interfering_with_witnesses_or_otherwise_obstructing_the_course_of_justice",
              "commiting_an_offence_whilst_on_bail",
              "for_that_persons_own_protection",
              "for_that_persons_own_welfare_or_own_interests_child_or_young_person_only"
            ]
          }
        },
        "condition": {
          "type": "string"
        },
        "reason": {
          "type": "string"
        }
      },
      "required": [
        "grounds",
        "condition",
        "reason"
      ]
    },

    "Offence": {
      "type": "object",
      "properties": {
        "type": { "enum": [ "Offence" ] },
        "from_date": {
          "type": "string"
        },
        "to_date": {
          "type": "string"
        },
        "offence_title": {
          "type": "string"
        },
        "charge_wording": {
          "type": "string"
        },
        "reply_to_charge": {
          "type": "string"
        },
        "points_to_prove_met_by": {
          "type": "string"
        },
        "demeanour_at_incident": {
          "type": "string"
        },
        "anticipated_plea": {
          "enum": [ "guilty", "not_guilty" ]
        },
        "committed_on_bail": {
          "type": "boolean"
        },
        "children_present": {
          "type": "boolean"
        },
        "ages_of_children": {
          "type": "array",
          "items": { "type": "string" }
        }
      },
      "required": [
        "type",
        "from_date",
        "to_date",
        "offence_title",
        "charge_wording",
        "points_to_prove_met_by",
        "anticipated_plea",
        "committed_on_bail"
      ],
      "additionalProperties": false
    },

    "GenericOffences": {
      "type": "object",
      "properties": {
        "type": { "enum": [ "GenericOffences" ] },
        "generic_offences": {
          "type": "array",
          "items": { "$ref": "#/definitions/GenericOffence" },
          "minItems": 1,
          "uniqueItems": true
        }
      },
      "required": [
        "genric_offences"
      ],
      "additionalProperties": false
    },

    "GenericOffence": {
      "properties": {
        "type": { "enum": [ "GenericOffence" ] },
        "cjs_offence_code": {
          "description": "Valid cjs code, not in any other group.",
          "type": "string"
        },
        "offence": {
          "$ref": "#/definitions/Offence"
        },
        "property_ids": {
          "type": "array",
          "items": { "type": "number" }
        },
        "victim_names": {
          "type": "array",
          "items": { "$ref": "#/definitions/PersonName" }
        }
      },
      "required": [
        "type",
        "cjs_offence_code",
        "offence"
      ],
      "additionalProperties": false
    },

    "AssaultOffences": {
      "type": "object",
      "properties": {
        "type": { "enum": [ "AssaultOffences" ] },
        "assault_offences": {
          "type": "array",
          "items": { "$ref": "#/definitions/AssaultOffence" },
          "minItems": 1,
          "uniqueItems": true
        }
      },
      "required": [
        "assault_offences"
      ],
      "additionalProperties": false
    },

    "AssaultOffence": {
      "properties": {
        "type": { "enum": [ "AssaultOffence" ] },
        "cjs_offence_code": {
          "enum": [
            "cj88001",
            "cj88116",
            "cj88001b",
            "cj88001a",
            "th68025",
            "pl96001",
            "pl96002",
            "pl96004",
            "cj91034",
            "cj91010",
            "ba07004",
            "tm04001",
            "ca05006",
            "of61095",
            "of61102a",
            "of61102",
            "of61131",
            "of61016",
            "of61017",
            "cd98073",
            "cd98070",
            "of61022",
            "of61127",
            "of61060",
            "of61126",
            "of61050",
            "of61059",
            "cy33049"
          ]
        },
        "offence": {
          "$ref": "#/definitions/Offence"
        },
        "assault_interview_extension": {
          "$ref": "#/definitions/AssaultInterviewExtension"
        },
        "victim_name": {
          "$ref": "#/definitions/PersonName"
        }

      },
      "required": [
        "type",
        "cjs_offence_code",
        "assault_interview_extension",
        "victim_name"
      ],
      "additionalProperties": false
    },

    "RetailTheftOffences": {
      "type": "object",
      "properties": {
        "type": { "enum": [ "RetailTheftOffences" ] },
        "retail_theft_offences": {
          "type": "array",
          "items": { "$ref": "#/definitions/RetailTheftOffence" },
          "minItems": 1,
          "uniqueItems": true
        }
      },
      "required": [
        "retail_theft_offences"
      ],
      "additionalProperties": false
    },

    "RetailTheftOffence": {
      "properties": {
        "type": { "enum": [ "RetailTheftOffence" ] },
        "cjs_offence_code": {
          "enum": [
            "h410"
          ]
        },
        "offence": {
          "$ref": "#/definitions/Offence"
        },
        "property_ids": {
          "type": "array",
          "items": { "type": "number" },
          "minItems": 1
        },
        "retail_theft_interview_extension": {
          "$ref": "#/definitions/RetailTheftInterviewExtension"
        }
      },
      "required": [
        "type",
        "cjs_offence_code",
        "property_ids",
        "retail_theft_interview_extension"
      ],
      "additionalProperties": false
    },

    "Property": {
      "type": "object",
      "properties": {
        "propery_id": {
          "type": "string"
        },
        "property_action": {
          "enum": [
            "stolen_not_recovered",
            "stolen_recovered",
            "stolen_recovered_damaged",
            "damaged"
          ]
        },
        "description": {
          "type": "string"
        },
        "value": {
          "type": "number"
        },
        "damage_description": {
          "type": "string"
        },
        "damage_cost": {
          "type": "number"
        },
        "recovered_from": {
          "type": "string"
        }
      },
      "required": [
        "propery_id",
        "property_action",
        "description",
        "value"
      ],
      "additionalProperties": false
    },

    "Injuries": {
      "properties": {
        "names_of_defendants_causing_injuries": {
          "$ref": "#/definitions/PersonName"
        },
        "description_of_injuries": {
          "type": "string"
        },
        "images_taken": {
          "type": "boolean"
        },
        "image_urls": {
          "type": "array",
          "items": { "type": "string" }
        },
        "images_description": {
          "type": "string"
        },
        "reason_photos_not_taken": {
          "type": "string"
        },
        "received_medical_treatment": {
          "type": "boolean"
        },
        "details_of_medical_treatment": {
          "type": "string"
        },
        "injuries_visible": {
          "type": "boolean"
        }
      },
      "required": [
        "names_of_defendants_causing_injuries",
        "description_of_injuries",
        "images_taken",
        "received_medical_treatment",
        "injuries_visible"
      ],
      "additionalProperties": false
    },

    "MmeNotRecordedResponse": {
      "type": "object",
